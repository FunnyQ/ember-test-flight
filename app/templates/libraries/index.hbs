<h2>內容列表</h2>

<div class="row">
  {{#each model as |library|}}
  <div class="col-md-4">
    {{#library-item item=library}}
      {{#link-to 'libraries.edit' library.id class='btn btn-success btn-xs'}}編輯{{/link-to}}
      <button class="btn btn-danger btn-xs" {{action 'deleteLibrary' library}}>刪除</button>
    {{/library-item}}
  </div>
  {{/each}}
</div>
